{
    "presets": [
        "react",
        "es2015",
        "stage-0"
    ],
    "plugins": [
        "transform-runtime",
        "add-module-exports",
        "transform-react-display-name"
    ],
    "env": {
        "test": {
            "plugins": [
                [
                    "react-transform",
                    {
                      "transforms": [
                        {
                          "transform": "react-transform-catch-errors",
                          "imports": ["react", "redbox-react"]
                        }
                      ]
                    }
                ]
            ]
        },
        "development": {
            "plugins": [
                [
                    "react-transform",
                    {
                      "transforms": [
                        {
                          "transform": "react-transform-catch-errors",
                          "imports": ["react", "redbox-react"]
                        }
                      ]
                    }
                ]
            ]
        }
    }
}
