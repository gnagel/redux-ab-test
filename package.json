  "scripts": {
    "test": "npm run test:js && npm run test:lint && npm run test:flow",
    "test:js": " NODE_PATH=src:./test ./node_modules/.bin/mocha",
    "test:js:watch": "./node_modules/.bin/mocha --watch",
    "test:lint": "./node_modules/.bin/eslint ./src",
    "test:flow": "./node_modules/.bin/flow check --show-all-errors",
    "start": "cd example && ./node_modules/.bin/webpack-dev-server",
    "clean": "rm -rf ./dist",
    "build": "babel --out-dir=./dist/ --ignore=./src/__test__ src/",
    "shipit": "npm run build && npm publish && npm run clean"
